{
  "manifest_version": 3,
  "name": "Wave",
  "description": "An extension that replaces the new tab page with animated waves and gradient backgrounds.",
  "version": "3.0.4.1",
  "icons": {
    "128": "icon_128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "search"
  ],
  "action": {
    "default_icon": "icon.png"
  }
}